                                    <i class="fas fa-globe fa-lg text-warning"></i>
                                    <p class="mb-0">https://mdbootstrap.com</p>
                                </li>
                                <li class="list-group-item d-flex justify-content-between align-items-center p-3">
                                    <a href="https://www.instagram.com/your_username_here">
                                        <i class="fab fa-instagram fa-lg" style="color: #ac2bac;"></i>
                                    </a>
                                    <p class="mb-0">Your Instagram Username</p>
                                </li>
                                <li class="list-group-item d-flex justify-content-between align-items-center p-3">
                                    <a href="https://www.twitter.com/your_username_here">
                                        <!-- Twitter -->
                                        <i class="fab fa-twitter fa-2x" style="color: #55acee;"></i>
                                    </a>
                                    <p class="mb-0">Your Twitter Username</p>
                                </li>
                                <li class="list-group-item d-flex justify-content-between align-items-center p-3">
                                    <a href="https://www.facebook.com/your_username_here">
                                        <!-- Facebook -->
                                        <i class="fab fa-facebook-f fa-2x" style="color: #3b5998;"></i>
                                    </a>
                                    <p class="mb-0">Your Facebook Username</p>
                                </li>
