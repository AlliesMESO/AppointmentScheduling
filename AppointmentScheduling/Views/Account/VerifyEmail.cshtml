@{
    ViewData["Title"] = "Verify Email";
}

<h1>Verify Your Email</h1>
<p>Click the button below to verify your email and complete the registration process.</p>

<form asp-action="CompleteVerification" asp-route-userId="@ViewData["UserId"]" asp-route-code="@ViewData["Token"]">
    <button type="submit">Verify Email</button>
</form>
